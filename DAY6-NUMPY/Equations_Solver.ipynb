   "source": [
    "import numpy as np\n",
    "\n",
    "# Coefficient matrix A\n",
    "A = np.array([[3, 2],\n",
    "              [7, -4]])\n",
    "\n",
    "# Right-hand side vector b\n",
    "b = np.array([16, 10])\n",
    "\n",
    "# Solve for x using np.linalg.solve()\n",
    "x = np.linalg.solve(A, b)\n",
    "\n",
    "print(\"Solution:\")\n",
    "print(f\"x = {x[0]}\")\n",
    "print(f\"y = {x[1]}\")\n"
   ]
  }
